#define _color2        #4e9a06
#define _color10       #4e9a06
!YEL Keyword
#define _color3        #f0dfaf
#define _color11       #f0dfaf
!BLU Number
#define _color4        #0087af
#define _color12       #0087af
!MAG Precondit
#define _color5        #0087af
#define _color13       #0087af
!CYN Float
#define _color6        #60dddd
#define _color14       #60dddd
!WHT Search
#define _color7        #efefef
#define _color15       #efefef
!FMT Include, StatusLine, ErrorMsg
#define _colorBD       #ffcfaf
#define _colorUL       #ccdc90
#define _colorIT       #80d4aa
!TXT Normal, Normal, Cursor
#define _foreground    #444444
#define _background    #eeeeee
#define _cursorColor   #444444
!URxvt*color0         : _color0
