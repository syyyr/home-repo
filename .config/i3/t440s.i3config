# T440s-specific configuration
exec --no-startup-id xinput set-prop "Synaptics TM2668-002" "libinput Natural Scrolling Enabled" 1
exec --no-startup-id xinput set-prop "Synaptics TM2668-002" "libinput Tapping Enabled" 1
exec --no-startup-id xinput set-prop "SynPS/2 Synaptics TouchPad" "libinput Natural Scrolling Enabled" 1
exec --no-startup-id xinput set-prop "SynPS/2 Synaptics TouchPad" "libinput Tapping Enabled" 1
exec --no-startup-id xinput set-prop "TPPS/2 IBM TrackPoint" "libinput Accel Speed" -0.5

bindsym --release XF86Search exec --no-startup-id "~/apps/copypasta.bash"
bindsym --release $mod+F12 exec --no-startup-id "~/apps/copypasta.bash"

